{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://comet.com/offline-system-info-msg-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "description": "The root schema comprises the entire JSON document.",
  "required": [
    "system_info"
  ],
  "properties": {
    "system_info": {
      "$id": "#/properties/system_info",
      "type": "object",
      "title": "The git_metadata Schema",
      "default": {},
      "required": [
        "key",
        "value"
      ],
      "properties": {
        "key": {
          "$id": "#/properties/system_info/properties/key",
          "type": [
            "number",
            "string"
          ],
          "title": "The Key Schema",
          "default": "",
          "examples": [
            "key"
          ]
        },
        "value": {
          "$id": "#/properties/system_info/properties/value",
          "type": [
            "number",
            "string"
          ],
          "title": "The Value Schema",
          "default": "",
          "examples": [
            "value",
            0
          ]
        }
      }
    },
    "message_id": {
      "$id": "#/properties/message_id",
      "type": "integer",
      "title": "Message ID",
      "description": "The ID of the message",
      "default": -1,
      "examples": [
        1
      ]
    }
  }
}